use company;

CREATE TABLE SALGRADE
(
  GRADE  int primary key,
  LOSAL  int,
  HISAL  int
);

desc salgrade ;

CREATE TABLE DEPT
(
  DEPTNO  int primary key,
  DNAME   VARCHAR(100),
  LOC     VARCHAR(100)
);

desc dept ;

CREATE TABLE EMP
(
  EMPNO     int primary key,
  ENAME     VARCHAR(100),
  JOB       VARCHAR(100),
  MGR       int,
  HIREDATE  DATE,
  SAL       decimal(7,2),
  COMM      decimal(7,2),
  DEPTNO    int 
);

desc emp ;