use company;

select length(ename) 
from emp; -- 1번

select length(ename) 
from emp 
where job = 'MANAGER'; -- 2번

select ename,job 
from emp 
where comm is null; -- 3번

select hiredate 
from emp 
where comm is not null; -- 4번

select substr(ename,2) 
from emp; -- 5번

select substr(job,1,3) 
from emp; -- 6번

select replace(ename,'K','P') from EMP;