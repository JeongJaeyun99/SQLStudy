use company;

CREATE TABLE DEPT
(
  DEPTNO  int,
  DNAME   VARCHAR(100),
  LOC     VARCHAR(100)
)

CREATE TABLE SALGRADE
(
  GRADE  int,
  LOSAL  int,
  HISAL  int
)

CREATE TABLE EMP
(
  EMPNO     int,
  ENAME     VARCHAR(100),
  JOB       VARCHAR(100),
  MGR       int,
  HIREDATE  DATE,
  SAL       decimal(7,2),
  COMM      decimal(7,2),
  DEPTNO    int
)