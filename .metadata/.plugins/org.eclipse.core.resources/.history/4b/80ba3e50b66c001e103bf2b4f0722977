select count(*)
from emp; -- 1ë²ˆ

select count(*)
from emp
where deptno = 20 and mgr <= 7700;

select min(sal)
from emp e 
where deptno = 10 or deptno = 20;

select min(sal),max(sal), sum(sal)
from emp;

select avg(sal)
from emp 
group by deptno;

select sum(sal)
from emp 
group by deptno
order by deptno;
