use shop2; 

select * from product3 p 
order by price desc; -- 2번

select name as '제품이름',content as '제품내용',price as '제품가격' 
from product3 p 
order by company; -- 3번

select distinct company 
from product3 p ; -- 4번

select price*5 as 'price5'
from product3 p;

select name,company
from product3 p
where price = 5000;




